class Unit {}
class Boolean {}
class Object {}

class Iterator
  Boolean hasNext()
  Object next()
  Unit remove()

class Collection
  Iterator iterator()

var Unit unit

class User
  Unit remove(Collection c, Object x)
    var Iterator i := c.iterator()
    while { hasNext := i.hasNext() }  (hasNext)
      y := i.next()
      if x == y { i.remove() }
    return unit

main
  unit := new
  var User u := new
  var Collection c := new
  var Object o := new
  c.add(o)
  u.remove(c, o)
