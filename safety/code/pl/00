class Unit {}
class Boolean {}
class Object {}

class Iterator
  Boolean hasNext()
  Object next()
  Unit remove()

class Collection
  Iterator iterator()

Unit unit

class User
  Iterator i

  Unit remove(Collection c, Object x)
    i := c.iterator()
    while { hasNext := i.hasNext() }  (hasNext)
      y := i.next()
      if x == y { i.remove() }
    return unit
